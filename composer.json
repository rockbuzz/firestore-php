{
    "name": "rockbuzz/firestore-php",
    "description": "Firestore PHP Client",
    "keywords": [
        "php",
        "firestore",
        "firebase",
        "google"
    ],
    "homepage": "https://github.com/rockbuzz/firestore-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Tiago Lopes",
            "email": "tiago@rockbuzz.com.br",
            "homepage": "https://rockbuzz.com.br"
        }
    ],
    "require": {
        "php": ">=5.6.6",
        "guzzlehttp/guzzle": "~7.0|~6.0|~5.0|~4.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "MrShan0\\PHPFirestore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrShan0\\PHPFirestore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"

    },
    "config": {
        "sort-packages": true
    }
}
